        <!-- jQuery -->
        <script src="{{ asset('jquery.js')}}"></script>
        <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
        <script src="https://code.jquery.com/jquery-3.1.1.js"></script>
